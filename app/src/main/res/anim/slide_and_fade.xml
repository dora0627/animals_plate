<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 透明度從 0 開始，漸變為 1 -->
    <objectAnimator
        android:propertyName="translationY"
        android:valueFrom="0"
        android:valueTo="-300"
        android:duration="500"
        android:interpolator="@android:interpolator/overshoot"
        />
</set>